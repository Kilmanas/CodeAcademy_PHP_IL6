{% extends "layout.html.twig" %}

{% block title %}Registracija{% endblock %}
{% block head %}
    {{ parent() }}
{% endblock %}
{% block content %}
    <div class="container">
<form action="create" method="post">
    <div class="row">

        <label for="1">Vardas</label><input type="text" name="name" id="1"></div>
    <div class="row"><label for="2">Pavardė</label><input type="text" name="last_name" id="2"></div>
    <div class="row"><label for="3">El. paštas</label><input type="email" name="email" id="3"></div>
    <div class="row"><label for="4">Slaptažodis</label><input type="password" name="password1" id="4"></div>
    <div class="row"><label for="5">Pakartokite slaptažodį</label><input type="password" name="password2" id="5"></div>
    <div class="row"><label for="6">Slapyvardis</label><input type="text" name="nickname" id="6"></div>
    <input type="submit" name="register" value="Registruotis">
    </form>
    </div>
{% endblock %}